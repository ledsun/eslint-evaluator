{
  "rules": {
    "quotes": [
      0,
      "single"
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "comma-dangle": 2,
    "no-unused-vars": 2,
    "no-console": 0,
    "no-dupe-keys": 2,
    "no-extra-semi": 2,
    "no-func-assign": 2,
    "no-unexpected-multiline": 2,
    "valid-typeof": 2,
    "no-constant-condition": 2,
    "no-control-regex": 2,
    "no-debugger": 2,
    "no-dupe-args": 2,
    "no-duplicate-case": 2,
    "no-empty-character-class": 2,
    "no-empty": 2,
    "no-ex-assign": 2,
    "no-extra-boolean-cast": 2,
    "use-isnan": 2,
    "no-cond-assign": 2,
    "no-inner-declarations": 2,
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 2,
    "no-negated-in-lhs": 2,
    "no-obj-calls": 2,
    "no-regex-spaces": 2,
    "no-sparse-arrays": 2,
    "no-unreachable": 2,
    "no-extra-parens": 2,
    "valid-jsdoc": 2
  },
  "env": {
    "node": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "ecmaFeatures": {
    "modules": true
  }
}
