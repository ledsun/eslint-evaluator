{
  "rules": {
    "quotes": [
      0,
      "single"
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "comma-dangle": 0,
    "no-unused-vars": 0,
    "no-console": 0,
    "no-dupe-keys": 2,
    "no-extra-semi": 2
  },
  "env": {
    "browser": true,
    "commonjs": true,
    "jquery": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "ecmaFeatures": {
    "modules": true
  },
  "globals": {
    "_": true,
    "toastr": true,
    "jsPlumb": true
  }
}
